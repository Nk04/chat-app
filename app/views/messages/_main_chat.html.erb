  <div class="main_up">
    <div class="main_up_left">
      <div class="room_title">お刺身交流会</div>
    </div>
    <div class=:main_up_right>
      <div class="chat_end">
        <a href="#">チャットを終了する</a>
      </div>
    </div>
  </div>
  <div class="main_center">
    <%= render partial: 'message', collection: @messages %>
  </div>
    <%= form_with model: [@room, @message], html: {class: "main_down"}, local: true do |f|%>
    <div class="form_message">
    <%= f.text_field :content, class: 'message_text', placeholder: 'type a message' %>
      <label class="form-image">
        <span class="image-file">画像</span>
        <%= f.file_field :image, class: 'hidden' %>
      </label>
    </div>
    <%= f.submit '送信', class: 'form_send' %>
  <% end %>
  </div>